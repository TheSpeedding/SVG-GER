PROGRAM Rectangles {
  SET_WIDTH(2).
  REPEAT 5 {
    DRAW_RECTANGLES(10).
    TURN_RIGHT.
    MOVE(125).
    TURN_LEFT.
  }  
}
FUNCTION DRAW_RECTANGLES(n) {  
  REPEAT n {
    DRAW_RECTANGLE(50).
    MOVE(75).
  }    
  TURN_LEFT.
  TURN_LEFT.
  MOVE(75*n).
  TURN_LEFT.
  TURN_LEFT.
}
FUNCTION DRAW_RECTANGLE(x) {
  SET_RANDOM_COLOR.
  PEN_DOWN.
  REPEAT 4 {
    MOVE(x).
    TURN_RIGHT.
  }
  PEN_UP.
}